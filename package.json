{
  "name": "inside_python_dict",
  "version": "1.0.0",
  "description": "Inside python dict - an explorable explanation",
  "main": "index.js",
  "scripts": {
    "test": "jest --env=node",
    "build:html": "mkdir -p build && (for i in {all,chapter1,chapter2,chapter3,chapter4}; do mustache src/mustache/$i.json src/page.html.template > build/$i.html; done)",
    "start": "npm run build:html && webpack-dev-server --config webpack.dev.js --host 0.0.0.0",
    "build:all": "npm run build:html && webpack --config webpack.prod.js",
    "babel-node": "npx babel-node --presets env,react --plugins transform-class-properties",
    "extractcode":"npm run babel-node extractPythonCode.js",
    "dictserver": "rm pynode.sock ; npm run pyReimplWrapper.js; rm pynode.sock",
    "precommit": "pretty-quick --staged"
  },
